.container{
  display: flex;
  flex-direction: column;
  overflow: hidden;
  height: 100vh;
  background-color: #eee;
  padding:10px;
}
.hyCon{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 12px 16px;
  background-color: #fff;
  border-radius: 10px;
}
.hyCon .t3{
  flex-basis: 60%;
  flex-shrink: 0;
  margin-right: 10px;
  font-size:12px;
  color: rgb(110, 110, 110);
}

.btn button{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 12px;
  background-color: seagreen;
  width: 100px !important;
  padding:8px 10px;
}

.photoCon{
  margin-top:10px;
  overflow: auto;
  display: flex;
  flex-wrap: wrap;
}

.photoCon .item{
  flex-basis: calc(33.3% - 10px);
  height:200rpx;
  border:1px solid #ccc;
  border-radius: 10px;
  margin:5px;
}

.photoCon .item.uploadItem{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color:#999;
  font-size:14px;
}

.photoCon .item image{
  width: 100%;
  height:100%;
  border-radius: 10px;
}